    if(ans == 5529){
        campaignTrail_temp.candidate_json[3]["fields"].first_name = "Orval";
        campaignTrail_temp.candidate_json[3]["fields"].color_hex = "#283EE1";
       campaignTrail_temp.candidate_json[0]["fields"].color_hex = "#CD272B";
        campaignTrail_temp.candidate_last_name = 'Lodge Jr.';
        campaignTrail_temp.candidate_image_url = 'https://i.imgur.com/HOprpJO.png';
        campaignTrail_temp.candidate_json[3]["fields"].last_name = "Faubus";
        campaignTrail_temp.candidate_json[0]["fields"].first_name = "Henry Cabot";
        campaignTrail_temp.candidate_json[3]["fields"].image_url = "https://i.imgur.com/CfnwAnC.png";
        campaignTrail_temp.candidate_json[0]["fields"].last_name = "Lodge Jr.";
        campaignTrail_temp.candidate_json[0]["fields"].image_url = "https://i.imgur.com/HOprpJO.png";
campaignTrail_temp.running_mate_image_url = 'https://i.imgur.com/sQ6lLBR.jpg';
campaignTrail_temp.running_mate_last_name = 'Ford';
    }

    if(ans == 5529){
        campaignTrail_temp.question_number = 34;
    }
}
